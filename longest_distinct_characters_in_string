#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    string s="abababcdefababcdab";
   
    map<char,int> mp;
   
   int c=0;
    for(int i=0;i<s.length();i++)
    {
        if(mp[s[i]]==0)
        {
            c++;
        cout<<s[i];
        
        }
        mp[s[i]]++;
    }
    
   
    
    cout<<"\nLongest distinct characters in string's count:"<<c;
     
     
     
     
    return 0;
}
