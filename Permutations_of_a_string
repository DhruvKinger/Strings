#include <iostream>
#include<bits/stdc++.h>
using namespace std;
void permute(string s)
{
    sort(s.begin(),s.end()); //Sort the string in lexicographically ascending order 
    
    do{
        cout<<s<<"\t";
    }while(next_permutation(s.begin(),s.end()));
}

int main()
{
    string s="CBAD";
    permute(s);
    return 0;
}
